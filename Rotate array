#User function Template for python3

class Solution:
    #Function to rotate an array by d elements in counter-clockwise direction. 
    def rotateArr(self, arr, d):
        #Your code here
        n= len(arr)
        d=d%n
        arr[:d] = arr[:d][::-1]
        arr[d:] = arr[d:][::-1]
        arr[:] = arr[::-1]
        
        return arr
